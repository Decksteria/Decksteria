<Project>
  <PropertyGroup>
    <TargetFramework Condition="'$(MSBuildProjectName)' != 'Decksteria.Ui.Maui'">net10.0</TargetFramework>
    <Nullable>enable</Nullable>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyVersion>$(ApplicationVersion)</AssemblyVersion>
    <FileVersion>$(ApplicationVersion)</FileVersion>
    <InformationalVersion>$(ApplicationVersion)</InformationalVersion>
  </PropertyGroup>
  <PropertyGroup>
    <RepositoryUrl>https://github.com/Decksteria/Decksteria</RepositoryUrl>
  </PropertyGroup>
</Project>
